<?xml version="1.0"?>
<addon scheme="3.0">
    <id>soneritics_tradetracker</id>
    <name>Soneritics Tradetracker</name>
    <version>1.2</version>
    <default_language>nl</default_language>
    <priority>10000</priority>
    <status>active</status>
    <has_icon>Y</has_icon>
    <settings>
        <sections>
            <section id="general">
                <items>
                    <item id="redirectdir">
                        <name>Redirect dir</name>
                        <type>input</type>
                        <default_value></default_value>
                    </item>
                    <item id="domain">
                        <name>Domain (w/o www)</name>
                        <type>input</type>
                        <default_value></default_value>
                    </item>
                    <item id="cid">
                        <name>Campaign ID</name>
                        <type>input</type>
                        <default_value></default_value>
                    </item>
                    <item id="pid">
                        <name>Product ID</name>
                        <type>input</type>
                        <default_value></default_value>
                    </item>
                    <item id="trackinggroupid">
                        <name>Tracking Group ID</name>
                        <type>input</type>
                        <default_value></default_value>
                    </item>
                </items>
            </section>
        </sections>
    </settings>
</addon>
